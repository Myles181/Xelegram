    "class-variance-authority": "^0.7.1",
    "cloudinary": "^1.33.0",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
